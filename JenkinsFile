pipeline {

    agent any
       stages {
                stage('Compile Stage') {
                    steps {
                        echo "clean compile"
                }
            }
             stage('testing stage') {
                     steps {
                        echo "clean test"
                }
            }

              stage('deployment stage') {
                  steps {
                    echo " cleanly deployed"
                    
            }
        } 
    }

  }